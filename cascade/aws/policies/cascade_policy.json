{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"dynamodb:PutItem"
			],
			"Resource": "arn:aws:dynamodb:us-east-2:*:table/CascadeReadings"
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:PutObject",
				"s3:ListBucket"
			],
			"Resource": [
				"arn:aws:s3:::cascade-reports",
				"arn:aws:s3:::cascade-reports/*"
			]
		}
	]
}